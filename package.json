{
	"name": "walnut-javascript",
	"version": "0.1.0",
	"scripts": {
		"dev-server": "http-server build -p 12080 -o index.html",
		"dev-copy": "copy-and-watch --watch example/** build",
		"livereload": "livereload build -p 35700 -w 50",
		"dev": "npm-run-all --parallel dev-*",
		"dev-live": "npm-run-all --parallel livereload dev-server dev-copy"
	},
	"license": "MIT",
	"devDependencies": {
		"copy-and-watch": "^0.1.5",
		"npm-run-all": "^4.1.5"
	}
}
